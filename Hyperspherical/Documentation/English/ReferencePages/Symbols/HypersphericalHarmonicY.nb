       RowBox[{"Abs", "[", "m", "]"}], ",", 
       RowBox[{"Abs", "[", "m", "]"}], ",", 
       RowBox[{"Cos", "[", "\[Theta]", "]"}]}], "]"}], " ", 
     SuperscriptBox[
      RowBox[{"Sin", "[", "\[Theta]", "]"}], 
      RowBox[{"Abs", "[", "m", "]"}]]}]}], ",", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"l", "|", "m"}], ")"}], "\[Element]", "Integers"}], "&&", 
