           RowBox[{"Abs", "[", "m", "]"}]}], ")"}], "!"}], " ", 
        RowBox[{"Gamma", "[", 
         RowBox[{"1", "+", "l", "+", 
          RowBox[{"Abs", "[", "m", "]"}]}], "]"}]}], 
       SuperscriptBox[
        RowBox[{"Gamma", "[", 
         RowBox[{"1", "+", "l"}], "]"}], "2"]]], " ", 
     RowBox[{"JacobiP", "[", 
      RowBox[{
       RowBox[{"l", "-", 
